msgid "%s: invalid argument for option -m\n"
msgstr "%s: 对于选项-m 参数无效\n"

#: pg_resetxlog.c:176
#, c-format
msgid "%s: multitransaction ID (-m) must not be 0\n"
msgstr "%s:  多事务 ID (-m) 不能为 0\n"

