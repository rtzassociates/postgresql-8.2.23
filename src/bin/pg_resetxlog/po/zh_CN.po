msgstr "%s: 事务 ID (-x) 不能为 0\n"

#: pg_resetxlog.c:155
#, c-format
msgid "%s: invalid argument for option -o\n"
msgstr "%s: 为 -o 选项的无效参数\n"

#: pg_resetxlog.c:161
#, c-format
msgid "%s: OID (-o) must not be 0\n"
msgstr "%s: OID (-o) 不能为 0\n"

#: pg_resetxlog.c:170
